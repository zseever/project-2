<script>
    function generateChart(stockId,labelsArr,dataArr) {
        new Chart(document.getElementById(stockId), {
        type: 'line',
        data:{
        labels: labelsArr,
                datasets: [{
                label: '',
                data: dataArr,
                backgroundColor: '#8fe590',
                borderColor: '#8fe590',
                borderWidth: 1
                }]
        },
        options: {
                plugins: {legend: false,},
                legend: {enabled: false,},
                scales: {
                        yAxes: [{
                        ticks: {
                        
                                min: 0,
                                max: 100,
                                callback: function(value){return value+ "%"}
                                },  
                                scaleLabel: {
                                display: false,
                                labelString: "Percentage"
                                }
                        }]
                },
                title: {
                display: false,
                text: 'Micronutrients',
                fontSize: 20
        },
        }
    }); 
    }

</script>
